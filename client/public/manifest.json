{
  "manifest_version": 3,
  "name": "Anti-Scatter",
  "version": "1.0",
  "description": "Description of your extension",
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],

  "action": {
    "default_popup": "popup/popup.html",
    "default_title": "Open the popup!"
  },
  "background": {
    "service_worker": "background/background.js"
  },
  "content_scripts": []
}
